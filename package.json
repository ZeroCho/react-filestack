{
  "name": "filestack-react",
  "version": "2.0.2",
  "main": "dist/filestack-react.js",
  "description": "React component wrapper for filestack-js",
  "scripts": {
    "test": "jest --watch --no-cache --coverage",
    "build": "rimraf dist && webpack --colors -p --display-error-details",
    "prepare": "npm run build",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filestack/filestack-react.git"
  },
  "keywords": [
    "filestack-react",
    "filestack",
    "filepicker",
    "react",
    "uploader",
    "upload",
    "file",
    "image",
    "ssr",
    "server"
  ],
  "author": "ZeroCho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/filestack/filestack-react/issues"
  },
  "homepage": "https://github.com/filestack/filestack-react#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.27.3",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "imports-loader": "^0.7.1",
    "jest-cli": "^23.1.0",
    "prop-types": "^15.7.1",
    "raw-loader": "^0.5.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-test-renderer": "^16.8.1",
    "rimraf": "^2.6.3",
    "style-loader": "^0.16.0",
    "webpack": "^2.3.2"
  },
  "peerDependencies": {
    "filestack-js": "^1.0.1",
    "react": "^16.4.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/tests/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__coverage__",
      "<rootDir>/__tests__/__mocks__",
      "<rootDir>/node_modules"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testRegex": "\\.(spec|e2e|snapshot)\\.js$",
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "<rootDir>/tests/__coverage__/",
    "coverageReporters": [
      "json",
      "html",
      "text"
    ],
    "browser": true,
    "moduleNameMapper": {
      "mock-data": "<rootDir>/tests/__mocks__/mockData.js",
      "^.+\\.(scss|css|svg|png)$": "identity-obj-proxy",
      "src": "<rootDir>/src"
    }
  },
  "dependencies": {
    "filestack-js": "^1.14.1"
  }
}
